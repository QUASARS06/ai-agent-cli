# Required
LLM_API_KEY=<your_api_key_here>

# Required: model name (LiteLLM format)
# Examples:
#   OpenAI:        gpt-4o-mini
#   OpenRouter:    openrouter/arcee-ai/trinity-large-preview:free
#   Gemini:        gemini/gemini-3-flash-preview

# LLM_MODEL=gemini/gemini-3-flash-preview
# LLM_MODEL=openrouter/arcee-ai/trinity-large-preview:free

LLM_MODEL=<your_model_name_here>

# Optional: OpenAI-compatible base URL
# (Only needed if using a proxy/gateway that mimics OpenAI API)
LLM_BASE_URL=

# Optional
LLM_TIMEOUT=60
